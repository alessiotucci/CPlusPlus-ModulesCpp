$(NAME)	Makefile	/^$(NAME): $(OBJ)$/;"	t
%.o	Makefile	/^%.o: %.cpp$/;"	t
CXX	Makefile	/^CXX = c++$/;"	m
CYAN	Makefile	/^CYAN := \\033[1;36m$/;"	m
DATA_HPP	Data.hpp	/^#define DATA_HPP$/;"	d
Data	Data.cpp	/^Data::Data(): prefix(0), suffix(0) { }$/;"	f	class:Data
Data	Data.cpp	/^Data::Data(const Data& other)$/;"	f	class:Data
Data	Data.cpp	/^Data::Data(int prefix, int suffix): prefix(prefix), suffix(suffix) { }$/;"	f	class:Data
Data	Data.hpp	/^class Data$/;"	c
FLAG	Makefile	/^FLAG = -Wall -Wextra -Werror -std=c++98$/;"	m
GREEN	Makefile	/^GREEN := \\033[1;32m$/;"	m
HEADERS	Makefile	/^HEADERS = Data.hpp \\$/;"	m
NAME	Makefile	/^NAME = serializer$/;"	m
RED	Makefile	/^RED := \\033[1;31m$/;"	m
RESET	Makefile	/^RESET := \\033[0m$/;"	m
SERIALIZER_HPP	Serializer.hpp	/^#define SERIALIZER_HPP$/;"	d
SOURCE	Makefile	/^SOURCE = main.cpp \\$/;"	m
Serializer	Serializer.cpp	/^Serializer::Serializer(){ }$/;"	f	class:Serializer
Serializer	Serializer.cpp	/^Serializer::Serializer(const Serializer& other)$/;"	f	class:Serializer
Serializer	Serializer.hpp	/^class Serializer$/;"	c
YELLOW	Makefile	/^YELLOW := \\033[1;33m$/;"	m
all	Makefile	/^all: $(NAME)$/;"	t
clean	Makefile	/^clean:$/;"	t
deserialize	Serializer.cpp	/^Data* Serializer::deserialize(uintptr_t raw)$/;"	f	class:Serializer	typeref:typename:Data *
fclean	Makefile	/^fclean: clean$/;"	t
hidden	Makefile	/^hidden:$/;"	t
main	main.cpp	/^int	main()$/;"	f	typeref:typename:int
operator <<	Data.cpp	/^std::ostream& operator<<(std::ostream& output, const Data param)$/;"	f	typeref:typename:std::ostream &
operator =	Data.cpp	/^Data& Data::operator=(const Data& other)$/;"	f	class:Data	typeref:typename:Data &
operator =	Serializer.cpp	/^Serializer& Serializer::operator=(const Serializer& other)$/;"	f	class:Serializer	typeref:typename:Serializer &
prefix	Data.hpp	/^	int prefix;$/;"	m	class:Data	typeref:typename:int
re	Makefile	/^re: fclean all$/;"	t
serialize	Serializer.cpp	/^uintptr_t Serializer::serialize(Data* ptr)$/;"	f	class:Serializer	typeref:typename:uintptr_t
suffix	Data.hpp	/^	int suffix;$/;"	m	class:Data	typeref:typename:int
~Data	Data.cpp	/^Data::~Data(){ }$/;"	f	class:Data
~Serializer	Serializer.cpp	/^Serializer::~Serializer(){ }$/;"	f	class:Serializer
